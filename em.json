{
  "name": "EmeraldsVector",

  "version": "0.3.0",

  "dependencies": {
    "EmeraldsBool": "The-Fourth-Wall/EmeraldsBool",
    "EmeraldsPreprocessor": "The-Fourth-Wall/EmeraldsPreprocessor"
  },

  "dev-dependencies": {
    "cSpec": "Oblivious-Oblivious/cSpec"
  },

  "build": "",

  "compile-flags": {
    "cc": "clang",
    "debug": {
      "opt": "-Og",
      "version": "-std=c11",
      "flags": "-g",
      "warnings": "-Wall -Wextra -Werror -pedantic -pedantic-errors -Wpedantic"
    },
    "release": {
      "opt": "-O2",
      "version": "-std=c2x",
      "flags": "",
      "warnings": ""
    },
    "test": {
      "opt": "-Og",
      "version": "-std=c11",
      "flags": "-g",
      "warnings": ""
    }
  },

  "license": "gpl-v3"
}
